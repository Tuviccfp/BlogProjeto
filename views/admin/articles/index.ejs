<%- include('../../partials/head.ejs') %>

<body>
    <%- include('../../partials/navbar.ejs') %>

    <div class="container">
        <table>
            <thead>
                <th>ID</th>
                <th>Title</th>
                <th>Slug</th>
                <th>Corpo</th>
                <th>Data de criação</th>
                <th>Última atualização</th>
                <th>Categoria</th>
                <th>Ações</th>
            </thead>

            <tbody>
                <% article.forEach((article) = > { %>
                    <th><%= article.id %></th>
                    <th><%= article.title %></th>
                    <th><%= article.slug %></th>
                    <th><%= article.body %></th>
                    <th><%= article.createdAt %></th>
                    <th><%= article.updatedAt %></th>
                <% }) %>
            </tbody>
        </table>
    </div>


</body>

<%- include('../../partials/footer.ejs') %>